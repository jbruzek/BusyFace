<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/watch_face_background"
        android:layout_width="@dimen/settings_preview_size"
        android:layout_height="@dimen/settings_preview_size"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/settings_watch_face_preview_background"/>

    <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:id="@+id/complications_view_wrapper">

        <ImageView
                android:id="@+id/v0_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/added_complication"
                android:background="@android:color/transparent"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"/>

        <ImageView
                android:id="@+id/v1_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/added_complication"
                android:background="@android:color/transparent"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/v0_bg"/>

        <ImageView
                android:id="@+id/v2_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/added_complication"
                android:background="@android:color/transparent"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/v1_bg"/>

        <ImageView
                android:id="@+id/v3_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/added_complication"
                android:background="@android:color/transparent"
                android:layout_alignParentTop="true"
                android:layout_toRightOf="@+id/v2_bg"/>

        <!-- next row -->

        <ImageView
                android:id="@+id/v4_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/added_complication"
                android:background="@android:color/transparent"
                android:layout_below="@+id/v0_bg"
                android:layout_alignParentStart="true"/>

        <ImageView
                android:id="@+id/v5_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/added_complication"
                android:background="@android:color/transparent"
                android:layout_below="@+id/v0_bg"
                android:layout_toRightOf="@+id/v4_bg"/>

        <ImageView
                android:id="@+id/v6_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/added_complication"
                android:background="@android:color/transparent"
                android:layout_below="@+id/v0_bg"
                android:layout_toRightOf="@+id/v5_bg"/>

        <ImageView
                android:id="@+id/v7_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/added_complication"
                android:background="@android:color/transparent"
                android:layout_below="@+id/v0_bg"
                android:layout_toRightOf="@+id/v6_bg"/>

        <!-- next row -->

